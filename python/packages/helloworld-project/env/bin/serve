#!/home/prism/devops/python/modules-pract/helloworld-project/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'helloworld','console_scripts','serve'
__requires__ = 'helloworld'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('helloworld', 'console_scripts', 'serve')()
    )
